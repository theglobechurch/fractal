{
  "env": {
    "browser": true
  },
  "globals": {
    "document": false,
    "Modernizr": false
  },
  "extends": "airbnb-base",
  "rules": {
    "strict": 0,
    "comma-dangle": ["error", "always-multiline"],
    "no-param-reassign": [2, {"props": false}],
    "space-before-function-paren": [2, "always"],
    "no-use-before-define": 0
  }
}
